---
const { theme: propTheme } = Astro.props;
const isMatrix = propTheme === 'matrix';
const isNES = propTheme === 'nes';
const isCyberpunk = propTheme === 'cyberpunk';
import Layout from '../layouts/Layout.astro'
import Hero from '../components/HeroSection/Hero.astro'
import Features from '../components/features/Features.astro'
import DevCollective from '../components/DevCollective.astro'
import Projects from '../components/Projects.astro'
import Contact from '../components/Contact.astro'
const theme = Astro.cookies.get("theme")?.value || "nes";
---


---

<Layout>
  <Hero theme={theme} />
  <DevCollective />
  <Features />
  <Projects />
  <Contact />
</Layout>
